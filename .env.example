# Telegram API настройки (получить на https://my.telegram.org)
TLG_APP_NAME=your_app_name
TLG_APP_API_ID=12345678
TLG_APP_API_HASH=your_api_hash_here

# Bot Token (получить у @BotFather)
I_BOT_TOKEN=1234567890:your_bot_token_here

# ID администраторов (через запятую)
TLG_ADMIN_ID_CLIENT=123456789,987654321

# Настройки базы данных
TYPE_DB=SQLITE
SETTINGS_DB_PATH=./data/settings.db

# Прокси настройки (опционально)
TLG_PROXY_SERVER=
TLG_PROXY_PORT=
TLG_PROXY_KEY=

# Логирование
LOG_LEVEL=INFO
LOG_FILE=./logs/tlgbotcore.log
